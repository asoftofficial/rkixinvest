{
    "name": "chwaqas/laramail",
    "description": "Laravel Mailable Editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Waqas",
            "email": "me@chwaqas.com",
            "homepage": "chwaqas.com"
        }
    ],
    "homepage": "https://github.com/asoft/laramail",
    "keywords": ["Laravel", "laramail", "mailEclipse", "mailable-editor", "laravel-mail", "laravel-mailable", "mailable", "markdown"],
    "require": {
        "illuminate/support": "^8.0",
        "reecem/mocker": "^1.1",
        "laravel/legacy-factories": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "~1.2",
        "orchestra/testbench": "^6.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Chwaqas\\Laramail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chwaqas\\Laramail\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chwaqas\\Laramail\\MailEclipseServiceProvider"
            ],
            "aliases": {
                "Laramail": "Chwaqas\\Laramail\\Facades\\Laramail"
            }
        }
    }
}
